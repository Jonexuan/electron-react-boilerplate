{
  "name": "electron-react-boilerplate",
  "version": "1.0.0",
  "main": "build/main.bundle.js",
  "repository": "https://github.com/kenlyau/electron-react-boilerplate.git",
  "author": "kenlyau <newallah@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev-renderer": "cross-env DEV=1 webpack serve --config ./script/webpack.config.renderer.js",
    "dev-webpack": "cross-env DEV=1  webpack --config ./script/webpack.config.js",
    "build-renderer": "webpack --config ./script/webpack.config.renderer.js",
    "dev": "cross-env START=1 npm run watch",
    "build": "rimraf build && concurrently \"npm run build-renderer\" \"webpack --config ./script/webpack.config.js\"",
    "start": "electron .",
    "watch": "concurrently \"npm run dev-renderer\" \"npm run dev-webpack\"",
    "compile-win32": "npm run build && electron-builder -w",
    "compile-darwin": "npm run build && electron-builder -m",
    "compile-linux": "npm run build && electron-builder -l",
    "rebuild": "electron-builder install-app-deps"
  },
  "devDependencies": {
    "@babel/core": "^7.13.15",
    "@electron/remote": "^1.1.0",
    "@types/chrome": "0.0.134",
    "@types/react": "17.0.3",
    "@types/react-dom": "17.0.3",
    "@types/rimraf": "^3.0.0",
    "awesome-node-loader": "^1.1.1",
    "babel-loader": "^8.2.2",
    "babel-plugin-dynamic-import-node": "^2.3.3",
    "concurrently": "^6.0.2",
    "copy-webpack-plugin": "^8.1.1",
    "cross-env": "7.0.3",
    "css-loader": "^5.2.6",
    "electron": "11.1.1",
    "electron-builder": "22.10.5",
    "electron-extensions": "^7.0.0-beta.3",
    "electron-updater": "4.3.8",
    "file-loader": "^6.2.0",
    "file-type": "16.3.0",
    "html-webpack-plugin": "^5.3.1",
    "jsonfile": "^6.1.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "rimraf": "^3.0.2",
    "style-loader": "^2.0.0",
    "terser": "^5.6.1",
    "terser-webpack-plugin": "^5.1.1",
    "webpack": "5.33.2",
    "webpack-bundle-analyzer": "^4.4.1",
    "webpack-cli": "4.6.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.7.3"
  },
  "dependencies": {
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/preset-env": "^7.14.5",
    "@babel/preset-react": "^7.14.5",
    "@babel/register": "^7.14.5",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.4.3",
    "antd": "3.26.3",
    "react-refresh": "^0.10.0"
  }
}
